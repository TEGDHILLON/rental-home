<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myhome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/homeicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/homeicon"
        android:supportsRtl="true">
        <activity android:name=".Payment"></activity>
        <activity android:name=".PaymentMode" />
        <activity android:name=".CheckOutActivity">
            <intent-filter>
                <action android:name="com.example.myHome.CheckOutActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".TownHouse" />
        <activity android:name=".Condominion" />
        <activity android:name=".SemiDetached" />
        <activity android:name=".Detached" /> <!-- android:theme="@style/AppTheme" -->
        <activity android:name=".Apartment" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>